# Generated by iptables-save v1.4.7 on Mon Jan 19 17:45:39 2015
*filter
:INPUT DROP [0:0]
:FORWARD DROP [0:0]
:OUTPUT DROP [0:0]
-A INPUT -s 203.187.177.128/27 -j ACCEPT 
-A INPUT -s 192.168.200.0/24 -j ACCEPT 
-A INPUT -i lo -j ACCEPT 
-A INPUT -p icmp -j ACCEPT
-A INPUT -p tcp -m state --state NEW -m tcp --dport 80 -j ACCEPT 
-A INPUT -p tcp -m state --state NEW -m tcp --dport 1080 -j ACCEPT 
-A INPUT -m state --state RELATED,ESTABLISHED -j ACCEPT 
-A OUTPUT -s 203.187.177.128/27 -j ACCEPT 
-A OUTPUT -s 192.168.200.0/24 -j ACCEPT 

COMMIT









iptables -A INPUT -s 203.187.177.128/27 -j ACCEPT
iptables -A INPUT -s 192.168.200.0/24 -j ACCEPT 

iptables -A OUTPUT -s 203.187.177.128/27 -j ACCEPT
iptables -A OUTPUT -s 192.168.200.0/24 -j ACCEPT 







