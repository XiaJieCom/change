body {
    font: 12px/150% Arial,Verdana,"微软雅黑";
    margin:0;

    background:#FFF;
}

#MainBodyNav td{
    width: 650px;
    height: 13px;
}
td{
    height: 50px;
}

#container {
    width:100%;
    margin: 0 auto;/* 两个值,第一个0代表上下边距为0,Auto代表自适应*/
}
#Head {
    width: 100%;;
    margin:0 auto;
    height:100px;


}
#h_total{
    width: 100%;
    height: 30px;
    background-color: #f5fafe;
    position: relative;
    top:10px;
    left:30px;
}
.nav li{
    display: inline;
    height: 60px;

}
.nav li a {
    display: inline-block;
    padding:0 3px;
    height:30px;
    line-height:30px;
    float: left;
}
#PageBody {
    width:100%;
    margin: 10px auto;
    height: 1090px;

}
#PageBodyNav {
    width:100%;
    height:50px;
    background: #b20101;
}
#LeftBody{
    width: 25%;
    height: 1080px;
    background: #f5fafe;
    float: left;
}
#LeftBodyNav{
    width: 70%;
    height: 450px;
    float: right;
}
#LeftBodyFoot{
    height: 50px;
    background: white;

}
#MainBody{
    width: 52%;
    height: 1150px;
    float: left;
}
#MainBodyHead{
    height: 50px;
    background: white;
}
#MainBodyNav{
    height: 120px;
}

#MainBodySort{
    height: 30px;
    background: greenyellow;
}
#MainTable table td img{
    height: 200px;
    width: 200px;
}
#MainTable table td{
    height: 200px;
    width: 200px;
}
#RightBody{
    width: 23%;
    height: 1070px;
    background: #f5fafe;
    float: left;
}
#RightBodyHead{
    height: 50px;
    margin: 10px;
}
#RightBodyPublic{
    height: 350px;
    margin: 10px;


}
#Footer {
    width:100%;
    margin:0 auto;
    height:80px;

    float: left;;
}